{
  "name": "bridge-and-seek",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist contracts/target contracts/codegenCache.json",
    "compile-contracts": "cd contracts && ${AZTEC_NARGO:-aztec-nargo} compile",
    "codegen-contracts": "cd contracts && ${AZTEC_BUILDER:-aztec} codegen ./target -o ./target",
    "copy-artifacts": "cp contracts/target/*.json contracts/target/*.ts src/artifacts",
    "build-contracts": "yarn clean && yarn compile-contracts && yarn codegen-contracts && yarn copy-artifacts",
    "deploy-contracts": "node --experimental-transform-types scripts/deploy.ts",
    "dev": "vite",
    "build-app": "node --max-old-space-size=14336 node_modules/vite/bin/vite.js build",
    "build-app:ci": "NODE_ENV=production node --max-old-space-size=6656 node_modules/vite/bin/vite.js build",
    "build": "yarn build-contracts && yarn build-app",
    "build:ci": "yarn build-contracts && yarn build-app:ci",
    "serve": "vite preview --port ${PORT:-3000}",
    "prep-test": "PROVER_ENABLED=false yarn deploy-contracts && yarn build-app",
    "test": "yarn prep-test && yarn playwright test",
    "lint": "prettier --check ./src"
  },
  "dependencies": {
    "@aztec/accounts": "1.1.2",
    "@aztec/aztec.js": "1.1.2",
    "@aztec/constants": "1.1.2",
    "@aztec/foundation": "1.1.2",
    "@aztec/kv-store": "1.1.2",
    "@aztec/noir-contracts.js": "1.1.2",
    "@aztec/pxe": "1.1.2",
    "@aztec/stdlib": "1.1.2",
    "@rainbow-me/rainbowkit": "2.2.8",
    "@tanstack/react-query": "5.85.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.8.0",
    "viem": "2.34.0",
    "wagmi": "2.16.4"
  },
  "devDependencies": {
    "@defi-wonderland/aztec-standards": "0.0.0-83476cda",
    "@playwright/test": "1.52.0",
    "@types/node": "22.15.17",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.1",
    "assert": "2.1.0",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.1",
    "prettier": "3.5.3",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "typescript": "5.3.3",
    "util": "0.12.5",
    "vite": "7.1.3",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-top-level-await": "1.6.0",
    "vite-plugin-wasm": "3.5.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "aztecVersion": "1.1.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
